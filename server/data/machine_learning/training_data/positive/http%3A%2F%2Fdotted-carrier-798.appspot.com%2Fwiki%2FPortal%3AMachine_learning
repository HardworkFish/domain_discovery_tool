	var anchor_top = $(".comments_anchor").offset().top;
	var anchor_height = $(".comments_anchor").height();
  var comments_top = $("#disqus_thread").offset().top;
	var top = $(this).scrollTop();
  var hScreen = $(window).height();
  gComments_height = $("#disqus_thread").height();
  thread_height = $("#disqus_thread").height();
